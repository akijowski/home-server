# https://coredns.io/manual/toc/
.:5300 {
   hosts /etc/coredns/hosts home.kijowski.io {
      fallthrough
   }
   # Quad9
   # https://coredns.io/plugins/forward/
   forward . tls://9.9.9.11 {
      tls_servername dns11.quad9.net
      health_check 5s
   }
   cache 30
   log home.kijowski.io
   errors
   health
   whoami
}
