{{ ansible_managed | comment }}

network:
    version: 2
    ethernets:
        all-eth:
            match:
                name: eth*
            dhcp4: false
            addresses:
                - {{ init_static_ip4 }}
            nameservers:
                addresses:
                    - 192.168.50.2
                    - 9.9.9.10
                    - 192.168.50.1
            routes:
                - to: default
                  via: 192.168.50.1
